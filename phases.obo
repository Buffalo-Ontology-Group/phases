property_value: IAO:0000114 "Published" xsd:string
property_value: isDefinedBy http://humanbehaviourchange.org/ontology/bcio.owl

[Term]
id: BCIO:010111
name: female gender
def: "A gender identity as being female." []
is_a: BCIO:015098 ! gender identity
property_value: example_of_usage "female, woman" xsd:string
property_value: IAO:0000114 "Published" xsd:string
property_value: IAO:0000118 "woman" xsd:string
property_value: isDefinedBy http://humanbehaviourchange.org/ontology/bcio.owl

[Term]
id: BCIO:010112
name: male gender
def: "A gender identity as being male." []
is_a: BCIO:015098 ! gender identity
property_value: example_of_usage "male, man" xsd:string
property_value: IAO:0000114 "Published" xsd:string
property_value: IAO:0000118 "man" xsd:string
