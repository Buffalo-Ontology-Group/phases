property_value: example_of_usage "full-day, 6 years" xsd:string
property_value: IAO:0000114 "Published" xsd:string
property_value: isDefinedBy http://humanbehaviourchange.org/ontology/bcio.owl

[Term]
id: BCIO:010127
name: affiliation to a formal group or organisation
