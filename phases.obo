property_value: isDefinedBy http://humanbehaviourchange.org/ontology/bcio.owl

[Term]
id: BCIO:010102
name: colleague
def: "A relatedness between person source and the target population that is a person with whom the participant works in a profession or business." []
is_a: BCIO:010094 ! relatedness between person source and the target population
property_value: IAO:0000114 "Published" xsd:string
property_value: isDefinedBy http://humanbehaviourchange.org/ontology/bcio.owl

[Term]
id: BCIO:010103
name: employer
