property_value: isDefinedBy http://humanbehaviourchange.org/ontology/bcio.owl

[Term]
id: BCIO:010098
name: sibling relationship
def: "A family member that is a family relationship between two persons with at least one shared parent." []
is_a: BCIO:010095 ! family member
property_value: example_of_usage "brother, sister, step-brother, step-sister" xsd:string
property_value: IAO:0000114 "Published" xsd:string
property_value: isDefinedBy http://humanbehaviourchange.org/ontology/bcio.owl

[Term]
id: BCIO:010099
name: child relationship
def: "A family member that is an offspring relationship from a person to their parent." []
is_a: BCIO:010095 ! family member
property_value: example_of_usage "daughter, son, step-daughter, step-son" xsd:string
property_value: IAO:0000114 "Published" xsd:string
property_value: isDefinedBy http://humanbehaviourchange.org/ontology/bcio.owl

[Term]
id: BCIO:010100
name: carer
def: "A relatedness between person source and the target population that is an individual who cares, unpaid, for a friend or family member who, due to illness or disability, requires support in their daily life activities." []
is_a: BCIO:010094 ! relatedness between person source and the target population
property_value: example_of_usage "carer" xsd:string
property_value: IAO:0000114 "Published" xsd:string
property_value: isDefinedBy http://humanbehaviourchange.org/ontology/bcio.owl

[Term]
id: BCIO:010101
