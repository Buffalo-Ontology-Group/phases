property_value: example_of_usage "\"Supportive Environment\" in the Integrated Theoretical Model for Alcohol and Other Drug Abuse Prevention" xsd:string
property_value: IAO:0000114 "Published" xsd:string
property_value: isDefinedBy http://humanbehaviourchange.org/ontology/bcio.owl

[Term]
id: BCIO:006091
