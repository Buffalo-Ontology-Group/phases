is_a: BFO:0000015 ! process
relationship: RO:0000057 BCIO:010002 ! has participant person source
property_value: example_of_usage "voluntary basis, volunteering" xsd:string
property_value: IAO:0000114 "Published" xsd:string
property_value: isDefinedBy http://humanbehaviourchange.org/ontology/bcio.owl

[Term]
id: BCIO:010130
name: payment of person source
