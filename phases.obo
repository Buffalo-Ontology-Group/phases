property_value: IAO:0000114 "Published" xsd:string
property_value: isDefinedBy http://humanbehaviourchange.org/ontology/bcio.owl

[Term]
id: BCIO:010115
name: religious group membership of person source
def: "A socio-demographic attribute of person source that is a religious group to which an individual identifies as belonging, where a religious group is a group of people characterised by the practice of a common religion." []
is_a: BCIO:010108 ! socio demographic attribute of person source
property_value: example_of_usage "Muslim, Christian, Hindu, Sikh, Buddhist, Judaism" xsd:string
property_value: IAO:0000114 "Published" xsd:string
property_value: isDefinedBy http://humanbehaviourchange.org/ontology/bcio.owl

[Term]
id: BCIO:010116
name: language proficiency of person source
