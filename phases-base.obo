property_value: terms:created "2025-03-03T21:39:49Z" xsd:dateTime

[Term]
id: PHASES:0002000
name: solitude
def: "A mental disposition that, if realized, is realized in being physical alone or in lacking social interactions, whether desired or not." [] {IAO:0000119="Oliveira, J., Guerra, S., Sousa, L., & Ribeiro, O. (2024). Solitude in old age: a scoping review of conceptualisations, associated factors and impacts. Aging & Mental Health, 29(2), 220–227. https://doi.org/10.1080/13607863.2024.2379408"}
is_a: MF:0000033
property_value: terms:contributor https://orcid.org/0000-0001-6906-5548
property_value: terms:created "2025-02-05T17:49:47Z" xsd:dateTime

[Term]
id: PHASES:0002001
name: gerotranscendence
def: "A mental disposition borne by older adults such that, if realized, is realized in shifting in goals, values, and perspectives towards connections to past and future generations and in seeking coherence in one's life narrative." [] {IAO:0000119="https://pmc.ncbi.nlm.nih.gov/articles/PMC10466938/"}
is_a: MF:0000033
property_value: terms:contributor https://orcid.org/0000-0001-6906-5548
property_value: terms:created "2025-02-05T17:54:30Z" xsd:dateTime

[Term]
id: PHASES:0002002
name: social isolation
def: "Social alienation that, if realized, is realized in being physically alone and desiring not to be." [] {IAO:0000119="https://www.nia.nih.gov/health/loneliness-and-social-isolation/loneliness-and-social-isolation-tips-staying-connected"}
is_a: BCIO:006014
property_value: terms:contributor https://orcid.org/0000-0001-6906-5548
property_value: terms:created "2025-02-26T21:16:23Z" xsd:dateTime

[Term]
id: PHASES:0002003
name: loneliness
def: "A subjective affective feeling of dissatisfaction with a lack of social interactions and relationships." [] {IAO:0000119="Asher, S. R., Hymel, S., & Renshaw, P. D. (1984). Loneliness in children. Child development, 1456-1464."}
is_a: MFOEM:000006
property_value: terms:contributor https://orcid.org/0000-0001-6906-5548
property_value: terms:created "2025-02-05T17:58:33Z" xsd:dateTime

[Term]
id: PHASES:0002004
name: theory
def: "An information content entity that is a principle or body of interrelated principles that purports to explain or predict a number of interrelated phenomena." [] {IAO:0000119="https://dictionary.apa.org/theory"}
is_a: IAO:0000030
