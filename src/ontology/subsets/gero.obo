property_value: IAO:0000602 "(forall (x y) (if (and (Continuant x) (exists (t) (hasContinuantPartOfAt y x t))) (Continuant y))) // axiom label in BFO2 CLIF: [126-001] " xsd:string {IAO:0010000="http://purl.obolibrary.org/obo/bfo/axiom/126-001", type="owl:Axiom"}
property_value: IAO:0000602 "(forall (x) (if (Continuant x) (Entity x))) // axiom label in BFO2 CLIF: [008-002] " xsd:string {IAO:0010000="http://purl.obolibrary.org/obo/bfo/axiom/008-002", type="owl:Axiom"}
